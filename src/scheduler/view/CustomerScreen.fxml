<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="375.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduler.view.CustomerScreenController">
   <children>
      <TableView fx:id="customerTbl" layoutX="14.0" layoutY="46.0" prefHeight="286.0" prefWidth="566.0">
        <columns>
            <TableColumn fx:id="IDClmn" prefWidth="75.0" text="Customer ID" />
          <TableColumn fx:id="nameClmn" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="addClmn" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="cityClmn" prefWidth="75.0" text="City" />
            <TableColumn fx:id="countryClmn" prefWidth="75.0" text="Country" />
            <TableColumn fx:id="phoneClmn" prefWidth="99.0" text="Phone Number" />
        </columns>
      </TableView>
      <Button fx:id="addBtn" layoutX="44.0" layoutY="340.0" mnemonicParsing="false" onAction="#addCustomer" text="Add" />
      <Button fx:id="editBtn" layoutX="96.0" layoutY="340.0" mnemonicParsing="false" onAction="#editCustomer" text="Edit" />
      <Button fx:id="deleteBtn" layoutX="150.0" layoutY="340.0" mnemonicParsing="false" onAction="#deleteCustomer" text="Delete" />
      <Button fx:id="apptBtn" layoutX="17.0" layoutY="14.0" mnemonicParsing="false" onAction="#apptView" text="Appointment Page" />
      <Button fx:id="reportBtn" layoutX="150.0" layoutY="14.0" mnemonicParsing="false" onAction="#reportView" text="Report Page" />
   </children>
</AnchorPane>
